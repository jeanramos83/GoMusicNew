@model List<GoMusicNew.Models.Musico>
<style type="text/css">
    body {
        background-color: beige
    }

    tbody {
        background-color: oldlace
    }
</style>
    <body>
        <h2>
            Musicos
            <a class="btn btn-sm btn-success float-right" asp-area="" asp-controller="Musico" asp-action="Inserir">
                Adicionar
            </a>
        </h2>

        <table class="table table-striped table-hover">
            <thead>
                <tr>
                    <th>
                        ID
                    </th>
                    <th>
                        Nome
                    </th>
                    <th>
                        Idade
                    </th>
                    <th>
                        Telefone
                    </th>
                    <th>
                        CPF
                    </th>
                    <th>
                        Email
                    </th>
                    <th>
                        Endereco
                    </th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>@item.IdMusico</td>
                        <td>@item.Nome</td>
                        <td>
                            @item.Idade
                        </td>
                        <td>
                            @item.Telefone
                        </td>
                        <td>
                            @item.CPF
                        </td>
                        <td>
                            @item.Email
                        </td>
                        <td>
                            <a href="http://localhost:50183/endereco/busca/@item.IdEndereco">@item.IdEndereco</a>
                        </td>
                        <td>
                            @Html.ActionLink("Delete", "Delete", new { id = item.IdMusico, id2 = item.IdEndereco })
                        </td>
                    </tr>
                }
            </tbody>
        </table>
    </body>